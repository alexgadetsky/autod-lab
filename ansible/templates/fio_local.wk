; Four threads, two query, two writers.

[global]
rw=randread
size=256m
directory=/tmp/fio-testing/data
ioengine=libaio
iodepth=4
invalidate=1
direct=1

[bgwriter]
rw=randwrite
iodepth=32

[queryA]
iodepth=1
ioengine=mmap
direct=0
thinktime=3

[queryB]
iodepth=1
ioengine=mmap
direct=0
thinktime=5

[bgupdater]
rw=randrw
iodepth=16
thinktime=100
size=32m

[VMPager1]
rw=randrw
iodepth=8
ioengine=sync
size=8G
blocksize=4K

[VMPager2]
rw=randrw:23
iodepth=8
ioengine=sync
size=8G
blocksize=4K
